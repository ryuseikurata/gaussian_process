   "source": [
    "\n",
    "using Distributions\n",
    "using Plots\n",
    "include(\"gaussian_kernel.jl\")\n",
    "\n",
    "gk = GaussianKernel(1)\n",
    "xs = collect(-4:0.5:4)\n",
    "gk_dist = MvNormal(zeros(Base.length(xs)), cov(gk, xs))\n",
    "Plots.plot(xs, rand(gk_dist, 5), \n",
    "    label = \"\", xlabel = \"x\", ylabel = \"f\",\n",
    "    linewidth = 2,\n",
    "    title = \"\")\n"
   ]
